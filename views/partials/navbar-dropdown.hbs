{{#each this.array}} {{#if this.name}}
<li class="dropdown-submenu">
    <a href="/search?term={{this.name}}&page=0" class="dropdown-toggle hl{{highlight}} hvr animate" data-toggle="dropdown">{{this.name}}</a>
    <ul class="dropdown-menu">
        {{>navbar-dropdown}}
    </ul>
</li>
{{else}}
<li>
    <a class="hl{{highlight}} hvr animate" href="/search?term={{this}}&page=0">{{this}}</a>
</li>
{{/if}} {{/each}}